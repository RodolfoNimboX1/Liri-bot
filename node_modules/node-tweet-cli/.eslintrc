// http://eslint.org/docs/rules
{
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "camelcase": 0,
    "new-cap": 0,
    "no-process-exit": 0,
    "no-shadow": 0,
    "consistent-return": 0
  }
}
